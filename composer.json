{
    "name": "julfiker/swagger-laravel",
    "description": "Api document based on swagger",
    "require": {
       "php": ">=5.5.9",
        "zircote/swagger-php": "^3.0"
    },
    "authors": [
        {
            "name": "Julfiker",
            "email": "mail.julfiker@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Julfiker\\Swagger\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish",
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    }
}
